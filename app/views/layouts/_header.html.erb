  <div class="navbar-inner">
    <div class="container">
      <%= link_to "SocioPub", '#', id: "logo" %>
      <nav>
	<ul class="nav pull-right">
          <% if logged_in? %>
          <%= link_to "Users", '#' %>
          <%= link_to "Profile", current_user %>
          <%= link_to "Settings", '#' %>
          <%= link_to "Log out", logout_path, method: "delete" %>
          <% else %>	  
	  <%= render 'sessions/new' %>
	  <% end %>

	</ul>
      </nav>
    </div>
  </div>
</header>


